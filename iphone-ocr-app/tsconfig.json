{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "moduleResolution": "bundler",
    "jsx": "react-native",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowJs": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
